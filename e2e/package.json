{
  "name": "@tryghost/e2e",
  "version": "0.0.0",
  "repository": "https://github.com/TryGhost/Ghost/tree/main/e2e",
  "author": "Ghost Foundation",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "dev": "tsc --watch --preserveWatchOutput --sourceMap",
    "build": "yarn build:ts",
    "build:ts": "tsc",
    "prepare": "tsc",
    "test": "playwright test",
    "test:factory": "playwright test --config=playwright-data-factory.config.mjs",
    "test:types": "tsc --noEmit",
    "lint": "eslint helpers --ext .ts --cache",
    "factory:create": "node lib/data-factory/scripts/create-test-post.js",
    "factory:play": "node lib/data-factory/scripts/factory-playground.js",
    "factory:repl": "node lib/data-factory/scripts/factory-repl.js",
    "factory:verify": "node lib/data-factory/scripts/verify-post.js"
  },
  "files": [
    "build"
  ],
  "devDependencies": {
    "@playwright/test": "1.53.2",
    "c8": "10.1.3",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "dependencies": {}
}
