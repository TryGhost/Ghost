{
  "editor.quickSuggestions": {
    "strings": true
  },
  "eslint.workingDirectories": [
    {
      "pattern": "./apps/*/"
    },
    {
      "pattern": "./ghost/*/"
    }
  ],
  "search.exclude": {
    "**/*.js.snap": true,
    "**/.git": true,
    "**/build/*": true,
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/ghost.map": true,
    "**/node_modules": true,
    "ghost/core/core/built/**": true,
    "ghost/web-analytics/.diff_tmp/**": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "git.detectSubmodules": false,
  "tinybird.dataProjectSubdir": "ghost/web-analytics",
  "tinybird.venv": ".venv"
}
