                    "state":"failure"
                },
                {
                    "argument":"ReplicationInstances[].ReplicationInstanceStatus",
                    "expected":"inaccessible-encryption-credentials",
                    "matcher":"pathAny",
                    "state":"failure"
                }
            ],
            "delay":60,
