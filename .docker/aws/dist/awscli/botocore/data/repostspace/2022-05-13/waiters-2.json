  "version" : 2,
  "waiters" : {
    "ChannelCreated" : {
      "delay" : 2,
      "maxAttempts" : 60,
      "operation" : "GetChannel",
      "acceptors" : [ {
        "matcher" : "path",
        "argument" : "channelStatus",
        "state" : "success",
        "expected" : "CREATED"
      }, {
        "matcher" : "path",
        "argument" : "channelStatus",
        "state" : "failure",
        "expected" : "CREATE_FAILED"
      }, {
        "matcher" : "path",
        "argument" : "channelStatus",
        "state" : "retry",
        "expected" : "CREATING"
      } ]
    },
