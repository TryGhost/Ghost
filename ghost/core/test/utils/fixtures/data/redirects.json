[
    {
        "from": "^/post/[0-9]+/([a-z0-9\\-]+)",
        "to": "/$1"
    },
    {
        "permanent": true,
        "from": "/my-old-blog-post/",
        "to": "/revamped-url/"
    },
    {
        "permanent": true,
        "from": "/test-params",
        "to": "/result?q=abc"
    },
    {
        "from": "^\\/what(\\/?)$",
        "to": "/what-does-god-say"
    },
    {
        "from": "^\\/search\\/label\\/([^\\%20]+)$",
        "to": "/tag/$1"
    },
    {
        "from": "^\\/topic\\/",
        "to": "/"
    },
    {
        "from": "^/resources\\/download(\\/?)$",
        "to": "/shubal-stearns"
    },
    {
        "from": "^\/(?:capture1|capture2)\/([A-Za-z0-9\\-]+)",
        "to": "/$1"
    },
    {
        "from": "^\\/[0-9]{4}\\/[0-9]{2}\\/([a-z0-9\\-]+)(\\.html)?(\\/)?$",
        "to": "/$1"
    },
    {
        "from": "^/prefix/([a-z0-9\\-]+)?",
        "to": "/blog/$1"
    },
    {
        "from": "/^\\/case-insensitive/i",
        "to": "/redirected-insensitive"
    },{
        "from": "^\\/Case-Sensitive",
        "to": "/redirected-sensitive"
    },
    {
        "from": "^\\/Default-Sensitive",
        "to": "/redirected-default"
    },
    {
        "from": "/external-url",
        "to": "https://ghost.org"
    },
    {
        "from": "/external-url/(.*)",
        "to": "https://ghost.org/$1"
    }
]
