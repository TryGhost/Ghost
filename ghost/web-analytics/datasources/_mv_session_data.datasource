VERSION 6

SCHEMA >
    `site_uuid` String,
    `session_id` String,
    `pageviews` UInt64,
    `first_pageview` DateTime,
    `last_pageview` DateTime,
    `duration` Int64,
    `is_bounce` Bool,
    `source` String

ENGINE "MergeTree"
ENGINE_PARTITION_KEY "toYYYYMM(first_pageview)"
ENGINE_SORTING_KEY "site_uuid, first_pageview, session_id"