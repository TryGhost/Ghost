    "targets": {
      "build:dev": {
        "dependsOn": [
          "build:dev",
          {
            "projects": [
              "@tryghost/admin-x-settings",
              "@tryghost/activitypub",
              "@tryghost/posts",
              "@tryghost/stats"
            ],
            "target": "build"
          }
        ]
      },
      "build": {
        "outputs": [
          "{projectRoot}/dist",
          "{workspaceRoot}/ghost/core/core/built/admin"
        ],
